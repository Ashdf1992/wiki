string GetPropNameByTag(ushort propId, ushort propType)
{
    uint key = ((propId as uint) << 2 * 8) + propType;

    if (key in PidTagPropertyMap)
        return PidTagPropertyMap[key];
    
    if (key in PidLidPropertyMap)
        return PidLidPropertyMap[key];

    return "<Unknown>";
}

string GetPropIdentity(ushort propId, ushort propType)
{
    return Format("{0} ({1})",
                      GetPropNameByTag(propId, propType),
                      Utility.DecToHexFormat(propId));
}

string GetPropIdentity(string propName, guid guidValue)
{
    string propSet = GetPropSetNameByGuid(guidValue);
    string key = "{" + propSet + "}::" + propName;
    
    return Format("{0} ({1}), PropertySet: {2}",
                      PidNamePropertyMap[key],
                      propName,
                      propSet) + ", " + guidValue.ToString();
}