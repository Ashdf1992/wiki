      <Metadata>
        <UniqueId>cc100d9d-ade0-43e4-b296-d12deeb115dc</UniqueId>
        <Name>File Sharing Errors</Name>
        <Description>Expose any SMB messages with the status != 0</Description>
        <Category>File Sharing and Authentication</Category>
        <Properties />
      </Metadata>
      <Content xsi:type="ColorRuleAsset">
        <Expression>
          (smb.status.code ~= 0 &amp;&amp; smb.Status.code ~= 0xC0000022) &amp;&amp;
          (smb2.header.status.code ~= 0 &amp;&amp; smb2.header.Status.code ~= 0xC0000022 &amp;&amp; smb2.header.Status.code ~= 0x80000006 &amp;&amp; smb2.header.Status.code ~= 0x103)
        </Expression>
        <Lines>Normal</Lines>
